<program> ::= <block> END

<block> ::= [ <statement> ]*

<if> ::= IF <condition> <block> ENDIF

<ifelse> ::= IF <condition> <block> [ ELSE <block>] ENDIF

<while> ::= WHILE <condition> <block> ENDWHILE

<loop> ::= LOOP <block> ENDLOOP

<repeat> ::= REPEAT <block> UNTIL <condition>

FOR <ident> = <expr1> TO <expr2> <block> ENDFOR